<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
         <link rel="stylesheet" href="{{ asset('bundles/vendors/bootstrap/bootstrap.css') }}"/>
         <link rel="stylesheet" href="{{ asset('bundles/css/style.css') }}"/>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    {% block body %}
        

    {% endblock %}
{% block javascripts %}
     <script src="{{ asset('bundles/vendors/jquery/jquery.js') }}"></script>
     <script src="{{ asset('bundles/vendors/bootstrap/bootstrap.js') }}"></script>
     <script src="{{ asset('bundles/js/script.js') }}"></script>
{% endblock %}
</body>
</html>
